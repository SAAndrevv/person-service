<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="liga.medical.personservice.core.repository.ContactMapper">


    <resultMap id="contactResulMap" type="Contact">
        <id property="id" column="id"/>
        <result property="phoneNumber" column="phone_number"/>
        <result property="email" column="email"/>
        <result property="profileLink" column="profile_link"/>
    </resultMap>

    <select id="findAllContact" resultMap="contactResulMap">
        select * from contact
    </select>
</mapper>