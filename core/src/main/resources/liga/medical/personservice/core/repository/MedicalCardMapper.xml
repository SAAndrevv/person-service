<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="liga.medical.personservice.core.repository.MedicalCardMapper">

    <resultMap id="medicalCardResulMap" type="MedicalCard">
        <id property="id" column="id"/>
        <result property="clientStatus" column="client_status"/>
        <result property="medStatus" column="med_status"/>
        <result property="registryDt" column="registry_dt"/>
        <result property="comment" column="comment"/>
    </resultMap>

    <select id="findAllMedicalCard" resultType="medicalCard">
        select * from medical_card
    </select>
</mapper>